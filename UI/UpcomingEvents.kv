<UpcomingEvents>:
    name: "upcoming_events"
    FloatLayout:
        size_hint: None, None

        Label:
            id: title
            text: "Upcoming Events"
            font_size: 50
            center_x: root.center_x
            center_y: root.center_y
            color: 0,0,1,1

        Button:
            id: return_button
            text: "Return"
            size: 75, 75
            center_x: root.width * 0.9
            center_y: root.height * 0.1
            on_press: root.pressed()
